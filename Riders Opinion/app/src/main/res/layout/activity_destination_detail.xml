<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/light_brown">

        <ImageView
            android:id="@+id/ivBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:src="@drawable/icon_back" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="8dp"
            android:src="@drawable/icon_search_white" />


        <TextView
            android:id="@+id/tvTitleToolbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="RIDING DESTINATIONS"
            android:textColor="@android:color/white"
            android:textSize="@dimen/title_text_size"
            android:textStyle="bold" />


        <ImageView
            android:id="@+id/ivMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:padding="8dp"
            android:src="@drawable/menu_icon" />

        <ImageView
            android:id="@+id/ivFilter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:padding="8dp"
            android:src="@drawable/icon_filter" />


    </android.support.v7.widget.Toolbar>

    <android.support.v4.widget.DrawerLayout
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_below="@id/toolbar">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">



        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/tvPlace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="12dp"
                    android:text="KASAULI"
                    android:textColor="@color/dark_brown"
                    android:textSize="@dimen/title_text_size" />

                <ImageView
                    android:id="@+id/ivStar1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@id/tvPlace"
                    android:layout_below="@id/tvPlace"
                    android:src="@drawable/icon_star_full_small" />

                <ImageView
                    android:id="@+id/ivStar2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/ivStar1"
                    android:layout_toRightOf="@id/ivStar1"
                    android:src="@drawable/icon_star_full_small" />

                <ImageView
                    android:id="@+id/ivStar3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/ivStar1"
                    android:layout_toRightOf="@id/ivStar2"
                    android:src="@drawable/icon_star_full_small" />

                <ImageView
                    android:id="@+id/ivStar4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/ivStar1"
                    android:layout_toRightOf="@id/ivStar3"
                    android:src="@drawable/icon_star_full_small" />

                <ImageView
                    android:id="@+id/ivStar5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/ivStar1"
                    android:layout_toRightOf="@id/ivStar4"
                    android:src="@drawable/icon_star_full_small" />

                <ImageView
                    android:id="@+id/ivFavorites"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignTop="@id/tvPlace"
                    android:padding="12dp"
                    android:src="@drawable/icon_add_favorites" />

                <ImageView
                    android:id="@+id/ivMap"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvPlace"
                    android:layout_toLeftOf="@id/ivFavorites"
                    android:padding="12dp"
                    android:src="@drawable/icon_map" />

                <ImageView
                    android:id="@+id/ivVideo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvPlace"
                    android:layout_toLeftOf="@id/ivMap"
                    android:padding="12dp"
                    android:src="@drawable/icon_video" />

                <TextView
                    android:id="@+id/tvReviews"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ivStar1"
                    android:layout_marginLeft="12dp"
                    android:text="" />

                <TextView
                    android:id="@+id/tvLike"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@id/ivVideo"
                    android:layout_below="@id/ivVideo"
                    android:layout_marginRight="4dp"
                    android:drawableLeft="@drawable/icon_like"
                    android:drawablePadding="8dp"
                    android:text="65" />

                <TextView
                    android:id="@+id/tvDislike"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/ivMap"
                    android:layout_marginLeft="4dp"
                    android:layout_toRightOf="@id/tvLike"
                    android:drawableLeft="@drawable/icon_dislike"
                    android:drawablePadding="8dp"
                    android:text="12" />


                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdvPostImage"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_below="@+id/tvDislike"
                    android:layout_marginTop="8dp"
                    app:placeholderImage="@color/twenty_of_black" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@id/sdvPostImage"
                    android:layout_alignParentRight="true"
                    android:layout_margin="8dp"
                    android:background="@color/light_brown"
                    android:drawablePadding="8dp"
                    android:drawableRight="@drawable/icon_like_white"
                    android:gravity="center"
                    android:padding="8dp"
                    android:text="LIKE"
                    android:textColor="@android:color/white" />

                <TextView
                    android:id="@+id/tvEatables"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/sdvPostImage"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_eating_joints"
                    android:padding="16dp"
                    android:text="12" />

                <TextView
                    android:id="@+id/tvPetrolPump"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvEatables"
                    android:layout_toRightOf="@id/tvEatables"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_petrol_pump"
                    android:padding="16dp"
                    android:text="18" />


                <TextView
                    android:id="@+id/tvServiceCenter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvEatables"
                    android:layout_toRightOf="@id/tvPetrolPump"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_mechanic"
                    android:padding="16dp"
                    android:text="8" />

                <TextView
                    android:id="@+id/tvFirstAid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvEatables"
                    android:layout_toRightOf="@id/tvServiceCenter"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_firstaid"
                    android:padding="16dp"
                    android:text="12" />

                <TextView
                    android:id="@+id/tvRides"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvEatables"
                    android:layout_toRightOf="@id/tvFirstAid"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_rides"
                    android:padding="16dp"
                    android:text="18" />


                <TextView
                    android:id="@+id/tvOffers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tvEatables"
                    android:layout_toRightOf="@id/tvRides"
                    android:drawablePadding="8dp"
                    android:drawableTop="@drawable/icon_offers"
                    android:padding="16dp"
                    android:text="8" />

                <LinearLayout
                    android:id="@+id/llAd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tvOffers"
                    android:layout_margin="8dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.facebook.drawee.view.SimpleDraweeView
                            android:id="@+id/sdvSponsoredAdImage1"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginRight="4dp"
                            app:placeholderImage="@color/twenty_of_black" />

                        <TextView
                            android:id="@+id/tvSponsoredAdTitle1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@id/sdvSponsoredAdImage1"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage1"
                            android:text="Hotel Whispering Winds"
                            android:textSize="11sp" />

                        <TextView
                            android:id="@+id/tvSponsoredAdReviews1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvSponsoredAdTitle1"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage1"
                            android:text="72 Reviews"
                            android:textSize="11sp" />


                        <ImageView
                            android:id="@+id/ivStar1Add1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews1"
                            android:layout_marginBottom="4dp"
                            android:layout_marginLeft="12dp"
                            android:layout_toRightOf="@id/tvSponsoredAdReviews1"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar2Add1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews1"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar1Add1"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar3Add1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews1"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar2Add1"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar4Add1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews1"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar3Add1"
                            android:src="@drawable/icon_star_half_small" />

                        <ImageView
                            android:id="@+id/ivStar5Add1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews1"
                            android:layout_marginBottom="4dp"
                            android:layout_marginRight="8dp"
                            android:layout_toRightOf="@id/ivStar4Add1"
                            android:src="@drawable/icon_star_empty_small" />


                        <TextView
                            android:id="@+id/tvSponsoredAdLocation1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvSponsoredAdReviews1"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage1"
                            android:text="Kasauli, India"
                            android:textSize="11sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <com.facebook.drawee.view.SimpleDraweeView
                            android:id="@+id/sdvSponsoredAdImage2"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginRight="4dp"
                            app:placeholderImage="@color/twenty_of_black" />

                        <TextView
                            android:id="@+id/tvSponsoredAdTitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage2"
                            android:text="Hotel Whispering Winds"
                            android:textSize="11sp" />

                        <TextView
                            android:id="@+id/tvSponsoredAdReviews2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvSponsoredAdTitle2"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage2"
                            android:text="72 Reviews"
                            android:textSize="11sp" />

                        <ImageView
                            android:id="@+id/ivStar1Add2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews2"
                            android:layout_marginBottom="4dp"
                            android:layout_marginLeft="12dp"
                            android:layout_toRightOf="@id/tvSponsoredAdReviews2"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar2Add2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews2"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar1Add2"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar3Add2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews2"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar2Add2"
                            android:src="@drawable/icon_star_full_small" />

                        <ImageView
                            android:id="@+id/ivStar4Add2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews2"
                            android:layout_marginBottom="4dp"
                            android:layout_toRightOf="@id/ivStar3Add2"
                            android:src="@drawable/icon_star_half_small" />

                        <ImageView
                            android:id="@+id/ivStar5Add2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/tvSponsoredAdReviews2"
                            android:layout_marginBottom="4dp"
                            android:layout_marginRight="8dp"
                            android:layout_toRightOf="@id/ivStar4Add2"
                            android:src="@drawable/icon_star_empty_small" />

                        <TextView
                            android:id="@+id/tvSponsoredAdLocation2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/tvSponsoredAdReviews2"
                            android:layout_toRightOf="@id/sdvSponsoredAdImage2"
                            android:text="Kasauli, India"
                            android:textSize="11sp" />

                    </RelativeLayout>


                </LinearLayout>

                <TextView
                    android:id="@+id/tvDesc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/llAd"
                    android:paddingBottom="8dp"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp"
                    android:paddingTop="8dp"
                    android:text="Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book" />
                <RelativeLayout
                    android:id="@+id/getdirectionlayout"
                    android:layout_below="@+id/tvDesc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp">

                    <ImageView
                        android:id="@+id/ivMap2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@drawable/icon_map_big" />

                    <TextView
                        android:id="@+id/tvPlace3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginBottom="8dp"
                        android:layout_toRightOf="@id/ivMap2"
                        android:drawableRight="@drawable/icon_get_direction"
                        android:text="CHANDIGARH" />

                    <View
                        android:id="@+id/viewLine2"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/light_brown"
                        android:layout_below="@id/tvPlace3"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="12dp"
                        android:layout_toRightOf="@id/ivMap2" />

                    <TextView
                        android:layout_marginTop="8dp"
                        android:id="@+id/tvPlace4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/viewLine2"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="12dp"
                        android:layout_marginBottom="8dp"
                        android:layout_toRightOf="@id/ivMap2"
                        android:drawableRight="@drawable/icon_get_direction"
                        android:text="SHIMLA" />


                </RelativeLayout>

                <View
                    android:id="@+id/view_direction"
                    android:layout_below="@+id/getdirectionlayout"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:background="@color/light_brown" />

                <fragment
                    android:id="@+id/map"
                    android:layout_marginTop="8dp"
                    android:name="com.google.android.gms.maps.MapFragment"
                    android:layout_width="match_parent"
                    android:layout_height="150dp"

                    android:layout_below="@+id/view_direction"/>


                <RelativeLayout
                    android:id="@+id/rv_properties"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/map"
                    >
                    <TextView
                        android:id="@+id/tv_Eatables"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginLeft="60dp"
                        android:drawablePadding="8dp"
                        android:drawableTop="@drawable/icon_eating_joints"
                        android:padding="16dp"
                        android:text="12" />

                    <TextView
                        android:id="@+id/tv_PetrolPump"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@id/tv_Eatables"
                        android:layout_toRightOf="@id/tv_Eatables"
                        android:drawablePadding="8dp"
                        android:drawableTop="@drawable/icon_petrol_pump"
                        android:padding="16dp"
                        android:text="18" />


                    <TextView
                        android:id="@+id/tv_ServiceCenter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@id/tv_Eatables"
                        android:layout_toRightOf="@id/tv_PetrolPump"
                        android:drawablePadding="8dp"
                        android:drawableTop="@drawable/icon_mechanic"
                        android:padding="16dp"
                        android:text="8" />

                    <TextView
                        android:id="@+id/tv_FirstAid"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@id/tv_Eatables"
                        android:layout_toRightOf="@id/tv_ServiceCenter"
                        android:drawablePadding="8dp"
                        android:drawableTop="@drawable/icon_firstaid"
                        android:padding="16dp"
                        android:text="12" />
                </RelativeLayout>


                <TextView
                    android:id="@+id/tvRouteReview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rv_properties"
                    android:layout_margin="12dp"
                    android:text="ROUTE REVIEW"
                    android:textColor="@color/light_brown" />

                <ImageView
                    android:id="@+id/ivGridView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/rv_properties"
                    android:padding="4dp"
                    android:src="@drawable/icon_grid_view_sel" />

                <ImageView
                    android:id="@+id/ivWeather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rv_properties"
                    android:layout_toLeftOf="@id/ivGridView"
                    android:padding="4dp"
                    android:src="@drawable/icon_image_view_sel" />

                <android.support.v4.view.ViewPager
                    android:id="@+id/viewPagerRecentPhotos"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:layout_below="@id/ivWeather"
                    android:layout_marginTop="8dp"
                    android:background="@color/twenty_of_black" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/viewPagerRecentPhotos"
                    android:layout_centerHorizontal="true"
                    android:padding="8dp"
                    android:text="12-Aug-2015"
                    android:textColor="@android:color/black" />


            </RelativeLayout>

        </ScrollView>
            <TextView
                android:id="@+id/tvSubmit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#8555"
                android:textColor="@android:color/white"
                android:text="BANNER 50 PX HEIGHT"
                android:gravity="center"
                android:textSize="22sp"
                android:padding="12dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lvSlidingMenu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="end"
            android:background="#8A3C16"
            android:clickable="true"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rlProfile"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@color/dark_brown"
                android:paddingBottom="8dp"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:paddingTop="8dp">
                <Button
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="9dp"
                    android:background="@drawable/icon_menu_edit"
                    android:layout_centerInParent="true"

                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="10dp"
                    />
                <Button
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="9dp"
                    android:background="@drawable/icon_menu_logout"
                    android:layout_centerInParent="true"

                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    />
                <TextView
                    android:id="@+id/tvName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:singleLine="true"
                    android:gravity="top|center"

                    android:layout_alignParentTop="true"
                    android:text="VISHAL SHAHI"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/title_text_size" />
                <com.facebook.drawee.view.SimpleDraweeView
                    android:id="@+id/sdvDp"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="7dp"
                    android:layout_below="@+id/tvName"
                    android:layout_centerInParent="true"
                    app:placeholderImage="@color/white"
                    app:roundAsCircle="true" />



                <!--  <TextView
                      android:id="@+id/tvAddress"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_alignLeft="@id/tvName"
